APP_TIMEZONE=America/Recife
JWT_SECRET=replace_with_a_secure_secret
JWT_EXPIRATION=3600000

# ===========================================
# KEYCLOAK CONFIGURATION
# ===========================================
# URL base do servidor Keycloak (sem /realms)
KEYCLOAK_AUTH_SERVER_URL=https://seukeycloak.com/auth
# Nome do realm no Keycloak
KEYCLOAK_REALM=testes
# Client ID da aplicação registrada no Keycloak
KEYCLOAK_CLIENT_ID=seu-client-id
# Client Secret - Retrieve from Keycloak Admin Console: Clients > Your Client > Credentials > Client Secret
# IMPORTANT: If your secret contains special characters (e.g., $), wrap it in single quotes
KEYCLOAK_CLIENT_SECRET=seu-client-secret
KEYCLOAK_REDIRECT_URI=http://localhost:8080/auth/login/oauth2/code/keycloak

# ===========================================
# LDAP / AD CONFIGURATION (Opcional)
# ===========================================
# LDAP Server URL
LDAP_URL=ldap://ldap.example.com:389
# Base DN
LDAP_BASE_DN=dc=example,dc=com
# User DN Pattern (leave empty to force manager search)
LDAP_USER_DN_PATTERN=CN={0},CN=Users
# Manager DN (service account for searches)
LDAP_MANAGER_DN=CN=manager,CN=Users,DC=example,DC=com
# Manager Password - NOTE: If password contains $ character, escape it with $$ or wrap in single quotes
LDAP_MANAGER_PASSWORD='replace_with_manager_password'
# Search filter
LDAP_SEARCH_FILTER=(sAMAccountName={0})
# Allowed groups (semicolon separated)
LDAP_ALLOWED_GROUPS=CN=GroupName,OU=Groups,DC=example,DC=com
# Admin group
LDAP_ADMIN_GROUP=CN=AdminGroup,OU=Groups,DC=example,DC=com

# ===========================================
# REACT APP CONFIGURATION
# ===========================================
# URL base do aplicativo React que receberá as informações de autenticação
REACT_APP_URL=http://localhost:5173/auth/login

PORT=8080

# Postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5435
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin
POSTGRES_DB=netnotify

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin
RABBITMQ_VHOST=/
RABBITMQ_EXCHANGE=fannout
RABBITMQ_EXCHANGE_DURABLE=false
RABBITMQ_QUEUE=notifications
RABBITMQ_ROUTING_KEY=

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=








